<div class ="forms">
    <section id="registration">
        {{#each errormessage}}
        <div>
            {{this}}
        </div>

        {{/each}}

        <form action="/registration" method="POST">
            <label for="r_user_id">Your Username</label><br>
            <input id="registration_id" type="text" name="registration_id"></input><br>
            <label for="email">Email</label><br>
            <input id="email" type="text" name="email" }></input><br>
            <label for="password">Password</label><br>
            <input id="password" type="password" name="password" }></input><br>
            <label for="c_password">Confirm Password</label><br>
            <input id="c_password" type="password" name="c_password" }></input><br>
            <input type="submit" value="Create Account">
        </form>
    </section>
</div>